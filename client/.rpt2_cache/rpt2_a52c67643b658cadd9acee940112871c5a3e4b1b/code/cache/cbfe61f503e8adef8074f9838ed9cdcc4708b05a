imgbg.width = 335;\r\n        imgbg.height = 95;\r\n        imgbg.x = pos.x;\r\n        imgbg.y = pos.y;\r\n        imgbg.sizeGrid = \"29,36,45,36\";\r\n        this.guideContainer.addChild(imgbg);\r\n        let label = new Laya.Label();\r\n        label.text = text;\r\n        imgbg.addChild(label);\r\n        label.fontSize = 26;\r\n        label.color = \"#923e06\";\r\n        label.height = imgbg.height;\r\n        label.width = imgbg.width;\r\n        label.valign = \"middle\";\r\n        label.align = \"center\";\r\n    }\r\n    ShowArrows(pos, url, rotation) {\r\n        let arrows = new Laya.Image();\r\n        arrows.skin = url;\r\n        arrows.x = pos.x;\r\n        arrows.y = pos.y;\r\n        arrows.rotation = rotation;\r\n        this.guideContainer.addChild(arrows);\r\n        return arrows;\r\n    }\r\n}\r\n//# sourceMappingURL=GuideView.js.map",
