  "code": "import { ui } from \"../../../ui/layaMaxUI\";\r\nimport { GameUIManager } from \"../../../manager/GameUIManager\";\r\nimport { Succulentpoint_Cfg } from \"../../../manager/ConfigManager\";\r\nimport { PotManager } from \"../../../manager/PotManager\";\r\nimport { PotState } from \"../../GameDefine\";\r\nimport { Time } from \"../../../common/Time\";\r\nimport { Utils } from \"../../../utils/Utils\";\r\nimport { Global } from \"../../../utils/Global\";\r\nimport { SceneManager } from \"../../../manager/SceneManager\";\r\nimport { DIYScene } from \"../../scene/DIYScene\";\r\nimport { DrawModel } from \"../DrawModel\";\r\nimport { FlowerpotSelView } from \"../Flowerpot/FlowerpotSelView\";\r\nimport { LoadingScenes1 } from \"../LoadingScenes1\";\r\nimport { GEvent } from \"../../../common/GEvent\";\r\nimport { GacEvent } from \"../../../common/GacEvent\";\r\nexport class PointFlowerStateView extends ui.view.Flowerpot.FlowerpotStateViewUI {\r\n    constructor(pointName) {\r\n        super();\r\n        this.curPottedId = 0;\r\n        this._pointName = \"\";\r\n        this._potData = [];\r\n        this._itemWidth = 463;\r\n        this._pointName = pointName[0];\r\n    }\r\n    onEnable() {\r\n        this.width = Laya.stage.width;\r\n        this.height = Laya.stage.height;\r\n        this.InitEvent();\r\n        this.InitView();\r\n        Global.sceneLock = true;\r\n    }\r\n    onDisable() {\r\n        Laya.timer.clearAll(this);\r\n    }\r\n    onDestroy() {\r\n        this.cBtn.offAll();\r\n        GEvent.RemoveEvent(GacEvent.OnPlantRipe, Laya.Handler.create(this, this.UpdatePotList));\r\n        Global.sceneLock = false;\r\n    }\r\n    onClose() {\r\n        GameUIManager.getInstance().destroyUI(PointFlowerStateView);\r\n    }\r\n    InitEvent() {\r\n        this.cBtn.on(Laya.Event.CLICK, this, this.onClose);\r\n        this.growBtn.on(Laya.Event.CLICK, this, this.OnGotoGrowView);\r\n        this.delBtn.on(Laya.Event.CLICK, this, this.OnDeletePot);\r\n        this.replaceBtn.on(Laya.Event.CLICK, this, this.OnReplacePot);\r\n        this.speedUpGrowBtn.on(Laya.Event.CLICK, this, this.OnSpeedUp);\r\n        this.editBtn.on(Laya.Event.CLICK, this, this.OnGotoEditPot);\r\n        this.leftBtn.on(Laya.Event.CLICK, this, this.OnClickLeft);\r\n        this.rightBtn.on(Laya.Event.CLICK, this, this.OnClickRight);\r\n        Laya.timer.loop(1000, this, this.UpdateTime);\r\n        GEvent.RegistEvent(GacEvent.OnPlantRipe, Laya.Handler.create(this, this.UpdatePotList));\r\n    }\r\n    InitView() {\r\n        this.SetPotList();\r\n    }\r\n    UpdatePotData() {\r\n        if (!this._pointName)\r\n            return;\r\n        let point = PotManager.getInstance().PotMap[this._pointName];\r\n        let potData = [];\r\n        this._tCurPot = null;\r\n        let nMax = PotManager.getInstance().GetPotMaxPost(this._pointName);\r\n        let nLength = 0;\r\n        if (point) {\r\n            if (point.PotList[PotManager.getInstance().GetPointCurPotIndex(this._pointName)]) {\r\n                this._tCurPot = point.PotList[PotManager.getInstance().GetPointCurPotIndex(this._pointName)];\r\n            }\r\n        }\r\n        for (let index = 0; index < nMax; index++) {\r\n            if (point && point.PotList[index]) {\r\n                potData.push(index);\r\n            }\r\n            else {\r\n                potData.push(-1);\r\n                nLength++;\r\n            }\r\n        }\r\n        this._potData = potData;\r\n        this.UpdateNum(nLength, nMax);\r\n    }\r\n    UpdateNum(nLength, nMax) {\r\n        this.la_valume.text = Utils.format(\"已存：{0}/{1}\", nMax - nLength, nMax);\r\n        if (nMax - nLength < nMax) {\r\n            this.la_valume.color = \"#269211\";\r\n            this.img_valumebg.skin = \"gameui/flowerstate/weicunman.png\";\r\n        }\r\n        else {\r\n            this.la_valume.color = \"#971616\";\r\n            this.img_valumebg.skin = \"gameui/flowerstate/anniu1.png\";\r\n        }\r\n    }\r\n    UpdatePotList() {\r\n        this.UpdatePotData();\r\n        this.timeItme = null;\r\n        this.TreeList.array = this._potData;\r\n        this.TreeList.refresh();\r\n        let tPot = this.GetPotted(this.curPottedId);\r\n        this.UpdateBtn(tPot ? tPot.State : null, 0);\r\n    }\r\n    UpdateTime() {\r\n        let tPot = this.GetPotted(this.curPottedId);\r\n        if (tPot && this.timeItme && tPot.State == PotState.Grow) {\r\n            let nPro = (Time.Seconds - tPot.GrowStartTime) / tPot.GrowTime;\r\n            this.timeItmepro.value = nPro;\r\n            let time = Utils.formatStandardTime(tPot.GrowTime - (Time.Seconds - tPot.GrowStartTime), false);\r\n            this.timeItme.text = Utils.format(\"正在成长 {0}\", time);\r\n        }\r\n    }\r\n    SetPotList() {\r\n        this.timeItme = null;\r\n        this.UpdatePotData();\r\n        this.TreeList.array = this._potData;\r\n        this.TreeList.renderHandler = new Laya.Handler(this, this.TreerenderHandler);\r\n        this.TreeList.hScrollBarSkin = \"\";\r\n        this.TreeList.scrollBar.hide = true;\r\n        this.TreeList.scrollBar.mouseWheelEnable = false;\r\n        this.TreeList.scrollBar.touchScrollEnable = false;\r\n        this.TreeList.scrollBar.changeHandler = new Laya.Handler(this, this.onScrollerChange);\r\n        this.onScrollerChange();\r\n        let tPot = this.GetPotted(0);\r\n        this.UpdateBtn(tPot ? tPot.State : null, 0);\r\n    }\r\n    TreerenderHandler(item, index) {\r\n        let tProGrow = item.getChildByName(\"pro_grow\");\r\n        let tGrowTime = item.getChildByName(\"la_growtime\");\r\n        let tScore = item.getChildByName(\"img_score\");\r\n        let tCost = item.getChildByName(\"la_cost\");\r\n        let tCostBg = item.getChildByName(\"la_costbg\");\r\n        let tPot = this.GetPotted(index);\r\n        tProGrow.visible = tPot && tPot.State == PotState.Grow;\r\n        tGrowTime.visible = tPot && tPot.State == PotState.Grow;\r\n        tScore.visible = tPot && tPot.State == PotState.Ripe;\r\n        tCost.visible = tPot && tPot.State == PotState.Ripe;\r\n        tCostBg.visible = tPot && tPot.State == PotState.Ripe;\r\n        if (tPot && tPot.State) {\r\n            if (tPot.State == PotState.Grow) {\r\n                let nPro = (Time.Seconds - tPot.GrowStartTime) / tPot.GrowTime;\r\n                tProGrow.value = nPro;\r\n                let time = Utils.formatStandardTime(tPot.GrowTime - (Time.Seconds - tPot.GrowStartTime), false);\r\n                tGrowTime.text = Utils.format(\"正在成长 {0}\", time);\r\n                this.timeItme = tGrowTime;\r\n                this.timeItmepro = tProGrow;\r\n            }\r\n            else if (tPot.State == PotState.Ripe) {\r\n                tScore.skin = tPot.GetQualityImg();\r\n                tCost.text = Utils.format(\"游客消费：{0}\", tPot.quality);\r\n            }\r\n        }\r\n        else {\r\n        }\r\n    }\r\n    OnUpdateFlowerModel(ui, index) {\r\n        if (this.flowerModel) {\r\n            this.flowerModel.Destroy();\r\n            this.flowerModel = null;\r\n        }\r\n        if (!PotManager.getInstance().PotMap[this._pointName] || !PotManager.getInstance().PotMap[this._pointName].PotList\r\n            || !PotManager.getInstance().PotMap[this._pointName].PotList[index]) {\r\n            this.flowerModel = new DrawModel();\r\n            this.flowerModel.bEmptyPot = true;\r\n            this.flowerModel.position = new Laya.Vector3(0, -0.3, -1.7);\r\n            this.flowerModel.scale = 1;\r\n            this.flowerModel.Start(ui);\r\n            return;\r\n        }\r\n        let tPoint = PotManager.getInstance().PotMap[this._pointName].PotList[index];\r\n        if (tPoint && tPoint.State == PotState.Grow) {\r\n            this.flowerModel = new DrawModel();\r\n            this.flowerModel.bEmptyBox = true;\r\n            this.flowerModel.position = new Laya.Vector3(0, -1.8, -5);\r\n            this.flowerModel.scale = 1;\r\n            this.flowerModel.Start(ui);\r\n            return;\r\n        }\r\n        this.flowerModel = new DrawModel();\r\n        this.flowerModel.flowerData = PotManager.getInstance().PotMap[this._pointName];\r\n        this.flowerModel.flowerPotStruct = PotManager.getInstance().PotMap[this._pointName].PointDataList[index];\r\n        this.flowerModel.bFlower = true;\r\n        let pos = PotManager.getInstance().scaleInfo[Succulentpoint_Cfg[this._pointName].type];\r\n        this.flowerModel.position = new Laya.Vector3(0, -pos[0], -pos[1]);\r\n        this.flowerModel.scale = 1.1;\r\n        this.flowerModel.Start(ui);\r\n    }\r\n    GetPotted(index) {\r\n        if (index < 0)\r\n            return;\r\n        let point = PotManager.getInstance().PotMap[this._pointName];\r\n        if (!point)\r\n            return;\r\n        if (point.PotList[index])\r\n            return point.PotList[index];\r\n        return;\r\n    }\r\n    UpdateBtn(state, index) {\r\n        let tPot = this.GetPotted(index);\r\n        this.growBtn.visible = false;\r\n        this.editPanel.visible = false;\r\n        this.speedUpGrowBtn.visible = false;\r\n        this.img_ripepot.visible = false;\r\n        this.growingBtn.visible = false;\r\n        this.ripegrowingBtn.visible = false;\r\n        if (!state) {\r\n            if (!this._tCurPot || this._tCurPot.State != PotState.Grow)\r\n                this.growBtn.visible = true;\r\n            this.growingBtn.visible = this._tCurPot && this._tCurPot.State == PotState.Grow;\r\n            this.img_ripepot.visible = true;\r\n            this.OnUpdateFlowerModel(this.img_ripepot, index);\r\n        }\r\n        else if (state == PotState.Grow) {\r\n            this.speedUpGrowBtn.visible = true;\r\n            this.img_ripepot.visible = true;\r\n            this.OnUpdateFlowerModel(this.img_ripepot, index);\r\n        }\r\n        else if (state == PotState.Ripe) {\r\n            this.editPanel.visible = true;\r\n            this.img_ripepot.visible = true;\r\n            this.ripegrowingBtn.visible = this._tCurPot && this._tCurPot.State == PotState.Grow;\r\n            this.replaceBtn.visible = !this.ripegrowingBtn.visible;\r\n            this.OnUpdateFlowerModel(this.img_ripepot, index);\r\n        }\r\n        let nCurIndex = PotManager.getInstance().GetPointCurPotIndex(this._pointName);\r\n        if (nCurIndex == index) {\r\n            this.usingBtn.visible = true;\r\n            this.replaceBtn.visible = false;\r\n        }\r\n        else {\r\n            this.usingBtn.visible = false;\r\n        }\r\n        this.potflag.skin = \"gameui/flowerstate/pot\" + Succulentpoint_Cfg[this._pointName].type + \".png\";\r\n    }\r\n    OnGotoGrowView() {\r\n        GameUIManager.getInstance().createUI(FlowerpotSelView, [this._pointName, this.curPottedId]);\r\n        this.onClose();\r\n    }\r\n    OnDeletePot() {\r\n        let tPot = this.GetPotted(this.curPottedId);\r\n        if (tPot && tPot.State != PotState.None) {\r\n            PotManager.getInstance().DelPot(this._pointName, this.curPottedId);\r\n        }\r\n        this.UpdatePotList();\r\n    }\r\n    OnSpeedUp() {\r\n        let tPot = this.GetPotted(this.curPottedId);\r\n        if (tPot && tPot.State == PotState.Grow) {\r\n            PotManager.getInstance().OnSpeedUp(this._pointName, this.curPottedId);\r\n        }\r\n        this.UpdatePotList();\r\n    }\r\n    OnReplacePot() {\r\n        PotManager.getInstance().ReplaceCurUse(this._pointName, this.curPottedId);\r\n        this.onClose();\r\n    }\r\n    OnGotoEditPot() {\r\n        let tPot = this.GetPotted(this.curPottedId);\r\n        this.onClose();\r\n        GameUIManager.getInstance().showUI(LoadingScenes1);\r\n        SceneManager.getInstance().openScene(DIYScene.instance, [this._pointName, this.curPottedId, 1], Laya.Handler.create(this, () => {\r\n        }));\r\n    }\r\n    OnClickLeft() {\r\n        this.curPottedId--;\r\n        if (this.curPottedId <= 0) {\r\n            this.curPottedId = 0;\r\n        }\r\n        let scrollV = this._itemWidth * this.curPottedId;\r\n        if (this._tween) {\r\n            Laya.Tween.clear(this._tween);\r\n        }\r\n        this._tween = Laya.Tween.to(this.TreeList.scrollBar, { value: scrollV }, 500, Laya.Ease.sineInOut);\r\n        let tPot = this.GetPotted(this.curPottedId);\r\n        this.UpdateBtn(tPot ? tPot.State : null, this.curPottedId);\r\n    }\r\n    OnClickRight() {\r\n        this.curPottedId++;\r\n        if (this.curPottedId >= this._potData.length) {\r\n            this.curPottedId = this._potData.length - 1;\r\n        }\r\n        let scrollV = this._itemWidth * this.curPottedId;\r\n        if (scrollV <= 0) {\r\n            scrollV = 0;\r\n        }\r\n        if (this._tween) {\r\n            Laya.Tween.clear(this._tween);\r\n        }\r\n        this._tween = Laya.Tween.to(this.TreeList.scrollBar, { value: scrollV }, 500, Laya.Ease.sineInOut);\r\n        let tPot = this.GetPotted(this.curPottedId);\r\n        this.UpdateBtn(tPot ? tPot.State : null, this.curPottedId);\r\n    }\r\n    onScrollerChange() {\r\n        if (this.curPottedId <= 0) {\r\n            this.leftBtn.visible = false;\r\n        }\r\n        else {\r\n            this.leftBtn.visible = true;\r\n        }\r\n        if (this.curPottedId >= this._potData.length - 1) {\r\n            this.rightBtn.visible = false;\r\n        }\r\n        else {\r\n            this.rightBtn.visible = true;\r\n        }\r\n    }\r\n}\r\n//# sourceMappingURL=PointFlowerStateView.js.map",
