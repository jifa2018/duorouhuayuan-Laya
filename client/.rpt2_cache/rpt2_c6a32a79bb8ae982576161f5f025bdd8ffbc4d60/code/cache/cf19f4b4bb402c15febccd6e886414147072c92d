nd() {\r\n        this.hand_left.visible = false;\r\n    }\r\n    hideUI() {\r\n        this.leftbt.visible = false;\r\n        this.rightbt.visible = false;\r\n        this.exitView.visible = true;\r\n    }\r\n    showUI() {\r\n        this.leftbt.visible = false;\r\n        this.rightbt.visible = false;\r\n        this.exitView.visible = false;\r\n    }\r\n}\r\n//# sourceMappingURL=SwitchScene.js.map",
