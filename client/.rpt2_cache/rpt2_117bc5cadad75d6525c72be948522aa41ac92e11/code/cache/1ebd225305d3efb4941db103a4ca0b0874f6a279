.mList.forEach((e, i) => {\r\n            e.array = arrDecorate;\r\n        });\r\n    }\r\n    renderList(cell, index) {\r\n        let img = cell.getChildByName(\"decorate_item\");\r\n        img.width = 100;\r\n        img.height = 100;\r\n        let imgSelect = cell.getChildByName(\"sign_selected\");\r\n        let imgUplock = cell.getChildByName(\"sign_uplock\");\r\n        img.skin = cell.dataSource.strStatueIcon;\r\n    }\r\n    eventList(e) {\r\n    }\r\n    getTabDecorate() {\r\n        let arr = [];\r\n        this.nStatue.forEach(element => {\r\n            if (element.StatueType == 2) {\r\n                arr.push(element);\r\n            }\r\n        });\r\n        return arr;\r\n    }\r\n}\r\n//# sourceMappingURL=IndexDecorateView.js.map",
